{
  "name": "goldilocks-e2e",
  "private": true,
  "version": "0.1.0",
  "description": "Cypress E2E tests for Goldilocks Flask app",
  "scripts": {
    "test:e2e": "cypress run --config-file config/cypress.config.js --browser electron",
    "test:e2e:headed": "cypress run --config-file config/cypress.config.js --browser electron --headed",
    "cypress:open": "cypress open --config-file config/cypress.config.js"
  },
  "devDependencies": {
    "cypress": "13.15.0"
  }
}
